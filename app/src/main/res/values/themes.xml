<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.Custom" parent="Theme.Material3.DayNight.NoActionBar">
<!--        todo add the base attributes,
                also the Light, Dark and PitchBlack theme should be applied
                in the same way the custom ones are applied to avoid repeating stuff (if possible)-->



        <!-- Toolbar, Status bar and Navigation bar -->
        <item name="android:windowContentOverlay">@null</item>
<!--        <item name="android:windowLightStatusBar">true</item>-->
<!--        <item name="android:windowLightNavigationBar" tools:targetApi="o_mr1">true</item>-->
        <item name="android:statusBarColor">?attr/colorSurface</item>
        <item name="android:navigationBarColor">?attr/colorSurface</item>

        <!-- Action Mode -->
        <item name="windowActionModeOverlay">true</item>

        <item name="android:windowDisablePreview">true</item>
<!--        <item name="colorControlNormal">#1B1B1F</item>-->
        <!-- FIXME: On android 13 with dynamic colors, this color won't use the theme's colors -->
        <item name="barcodeScannerActionBackgroundColor">#80DAE2FF</item>
    </style>

    <style name="Theme.Light" parent="Theme.Material3.Light.NoActionBar">
        <!-- Primary brand color -->
        <item name="colorPrimary">#0956CF</item>
        <item name="colorOnPrimary">#FFFFFF</item>
        <item name="colorPrimaryContainer">#DAE2FF</item>
        <item name="colorOnPrimaryContainer">#001848</item>

        <!-- Secondary brand color -->
        <item name="colorSecondary">#245FA6</item>
        <item name="colorOnSecondary">#FFFFFF</item>
        <item name="colorSecondaryContainer">#D5E3FF</item>
        <item name="colorOnSecondaryContainer">#001B3C</item>

        <!-- Tertiary brand color -->
        <item name="colorTertiary">#255EA7</item>
        <item name="colorOnTertiary">#FFFFFF</item>
        <item name="colorTertiaryContainer">#D5E3FF</item>
        <item name="colorOnTertiaryContainer">#001B3C</item>

        <!-- Error -->
        <item name="colorError">#BA1A1A</item>
        <item name="colorErrorContainer">#FFDAD6</item>
        <item name="colorOnError">#FFFFFF</item>
        <item name="colorOnErrorContainer">#410002</item>

        <!-- Surface & background -->
        <item name="android:windowBackground">#FEFBFF</item>
        <item name="android:colorBackground">#FEFBFF</item>
        <item name="colorOnBackground">#FEFBFF</item>
        <item name="colorSurface">#FEFBFF</item>
        <item name="colorOnSurface">#1B1B1F</item>
        <item name="colorSurfaceVariant">#E1E2EC</item>
        <item name="colorOnSurfaceVariant">#45464F</item>
        <item name="colorOutline">#757780</item>

        <!-- Toolbar, Status bar and Navigation bar -->
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:windowLightNavigationBar" tools:targetApi="o_mr1">true</item>
        <item name="android:statusBarColor">?attr/colorSurface</item>
        <item name="android:navigationBarColor">?attr/colorSurface</item>

        <!-- Action Mode -->
        <item name="windowActionModeOverlay">true</item>

        <item name="android:windowDisablePreview">true</item>
        <item name="colorControlNormal">#1B1B1F</item>
        <!-- FIXME: On android 13 with dynamic colors, this color won't use the theme's colors -->
        <item name="barcodeScannerActionBackgroundColor">#80DAE2FF</item>
    </style>

